
<div class="row">
  <ul class="list-inline col-md-5">
    <li class="list-inline-item"><%= link_to '新着順', photoposts_path  %></li>
    <li class="list-inline-item"><%= link_to '人気順', ranking_photoposts_path %></li>
    <li class="list-inline-item"><%= link_to "犬の写真",category_photoposts_path(category: 0) %></li>
    <li class="list-inline-item"><%= link_to "猫の写真",category_photoposts_path(category: 1)  %></li>
    <li class="list-inline-item"><%= link_to "その他の写真",category_photoposts_path(category: 2)  %></li>
  </ul>
  <div class="col-md-4">
    <%= form_with url: search_photoposts_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.submit '検索' %>
    <% end %>
  </div>
  <div class="col-md-3 text-right">
    <%= link_to "新規投稿", new_photopost_path, class: 'btn btn-primary' %>
  </div>
</div>
<h1 class="text-center mt-4 mb-4">投稿一覧</h1>
  <%= render 'feed_photoposts', photoposts: @photoposts %>
  <%= paginate @photoposts %>
  